<table>
  <thead>
    <tr>
      <th>Mission ID</th>
      <th>Client</th>
      <th>Architeture</th>
      <th>Central VANT</th>
      <th>Formation</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    {{#each missions}}
    <tr>
      <td>{{this.id}}</td>
      <td>{{this.client}}</td>
      <td>{{this.architecture}}</td>
      <td>{{this.centralVant}}</td>
      <td>{{this.formation}}</td>
      <td><a href="/missions/{{this.id}}"><i class="fas fa-eye" aria-hidden="true"></i></a></td>
      <td><a href="/missions/{{this.id}}/edit"><i class="fas fa-edit" aria-hidden="true"></i></a></td>
      <td><a href="/missions/{{this.id}}/delete" data-confirm="Are you sure?"><i class="fas fa-trash"
            aria-hidden="true"></i></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>

<br>
<a href="/missions/new">New Mission</a>