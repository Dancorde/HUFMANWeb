<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1 class="m-0 text-dark">VANTs</h1>
      </div>
    </div>
  </div>
</div>

<div class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="card card-primary card-outline">
          <div class="row">
            <div class="card-body box-profile">
              <div class="row">
                <div class="col-md-6">
                  <p>typeId: {{vant.typeId}}</p>
                  <p>valueId: {{vant.valueId}}</p>
                  <p>model: {{vant.model}}</p>
                  <p>type: {{vant.type}}</p>
                </div>
                <div class="col-md-6">
                  <p>manufacturer: {{vant.manufacturer}}</p>
                  <p>comunication: {{vant.comunication}}</p>
                  <p>tecType: {{vant.tecType}}</p>
                  <p>tecAddress: {{vant.tecAddress}}</p>
                </div>
              </div>
              <div class="text-center">
                <a class="btn btn-warning" href="/vants/{{vant.id}}/edit"><i class="fas fa-edit" aria-hidden="true"></i> Edit</a>
                <a class="btn btn-danger" href="/vants/{{vant.id}}/delete" onclick="return confirm('Are you sure?')"><i
                    class="fas fa-trash" aria-hidden="true"></i> Delete</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

