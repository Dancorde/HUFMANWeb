<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>typeId</th>
      <th>valueId</th>
      <th>model</th>
      <th>type</th>
      <th>manufacturer</th>
      <th>comunication</th>
      <th>tecType</th>
      <th>tecAddress</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    {{#each vants}}
    <tr>
      <td>{{this.id}}</td>
      <td>{{this.typeId}}</td>
      <td>{{this.valueId}}</td>
      <td>{{this.model}}</td>
      <td>{{this.type}}</td>
      <td>{{this.manufacturer}}</td>
      <td>{{this.comunication}}</td>
      <td>{{this.tecType}}</td>
      <td>{{this.tecAddress}}</td>
      <td><a href="/vants/{{this.id}}"><i class="fas fa-eye" aria-hidden="true"></i></a></td>
      <td><a href="/vants/{{this.id}}/edit"><i class="fas fa-edit" aria-hidden="true"></i></a></td>
      <td><a href="/vants/{{this.id}}/delete" data-confirm="Are you sure?"><i class="fas fa-trash"
            aria-hidden="true"></i></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>

<br>
<a href="/vants/new">New VANT</a>