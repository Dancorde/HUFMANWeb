<table>
  <thead>
    <tr>
      <th>compVantId</th>
      <th>typeId</th>
      <th>valueId</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    {{#each compVants}}
    <tr>
      <td>{{this.compVantId}}</td>
      <td>{{this.typeId}}</td>
      <td>{{this.valueId}}</td>
      <td>{{this.name}}</td>
      <td><a href="/compvants/{{this.compVantId}}"><i class="fas fa-eye" aria-hidden="true"></i></a></td>
      <td><a href="/compvants/{{this.compVantId}}/edit"><i class="fas fa-edit" aria-hidden="true"></i></a></td>
      <td><a href="/compvants/{{this.compVantId}}/delete" data-confirm="Are you sure?"><i class="fas fa-trash"
            aria-hidden="true"></i></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>

<br>
<a href="/compvants/new">New CompVant</a>