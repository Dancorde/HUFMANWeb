<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1 class="m-0 text-dark">CompVANTs</h1>
      </div>
    </div>
  </div>
</div>

<div class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="card card-primary">
          <div class="card-header">
            <h3 class="card-title">Edit CompVANT</h3>
          </div>
          <form action="/compvants/{{compVant.compVantId}}" method="post">
            <div class="card-body">
              <div class="form-group">
                <label for="typeId">typeId</label>
                <input class="form-control" type="text" name="typeId" id="typeId" value="{{compVant.typeId}}">
              </div>
              <div class="form-group">
                <label for="valueId">valueId</label>
                <input class="form-control" type="text" name="valueId" id="valueId" value="{{compVant.valueId}}">
              </div>
              <div class="form-group">
                <label for="name">name</label>
                <input class="form-control" type="text" name="name" id="name" value="{{compVant.name}}">
              </div>
            </div>
            <div class="card-footer">
              <button class="btn btn-primary" type="submit">Save</button>
              <button class="btn btn-danger" onclick="window.history.back();">Cancel</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>