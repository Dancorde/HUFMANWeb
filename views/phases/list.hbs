<table>
  <thead>
    <tr>
      <th>Mission ID</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    {{#each phases}}
    <tr>
      <td>{{this.missionId}}</td>
      <td>{{this.name}}</td>
      <td><a href="/phases/{{this.id}}"><i class="fas fa-eye" aria-hidden="true"></i></a></td>
      <td><a href="/phases/{{this.id}}/edit"><i class="fas fa-edit" aria-hidden="true"></i></a></td>
      <td><a href="/phases/{{this.id}}/delete" data-confirm="Are you sure?"><i class="fas fa-trash"
            aria-hidden="true"></i></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>

<br>
<a href="/phases/new">New Phase</a>