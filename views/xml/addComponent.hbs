<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1 class="m-0 text-dark">Add Component to Object</h1>
      </div>
    </div>
  </div>
</div>

<div class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="card">
          <div class="card-header">
            <h3 class="card-title">Interfaces</h3>
          </div>
          <form action="/xml/addComponent" method="post">
            <div class="card-body">
              <form action="/xml/addComponent" method="post">
                <div class="card-body">
                  <div class="form-group">
                    <label for="classification">Classification</label>
                    <select class="form-control" type="text" name="classification" id="classification">
                      <option value="REQUIRED">Required</option>
                      <option value="MISSION_SPECIFIC">Mission Specific</option>
                    </select>
                  </div>
                  <div class="form-group">
                    <label for="emergencyState">Emergency State</label>
                    <select class="form-control" name="emergencyState" id="emergencyState">
                      <option value="ON">ON</option>
                      <option value="OFF">OFF</option>
                    </select>
                  </div>
                </div>
                <div class="card-footer">
                  <button class="btn btn-primary" type="submit">Add</button>
                </div>
              </form>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>